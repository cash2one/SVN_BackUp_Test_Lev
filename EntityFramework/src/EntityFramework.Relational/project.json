{
  "version": "7.0.0-*",
  "description": "Shared Entity Framework components for relational data stores.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "EntityFramework.Core": "7.0.0-*",
    "System.Diagnostics.Tracing.Telemetry": "4.0.0-beta-*"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "EntityFramework.Relational.RelationalStrings": "Properties/RelationalStrings.resx"
  },
  "resource": ["Properties/EntityFramework.Relational.rd.xml"],
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Transactions": "4.0.0.0"
      }
    },
    "dnxcore50": {
      "dependencies": {
        "System.Data.Common": "4.0.1-beta-*",
        "System.Text.RegularExpressions": "4.0.11-beta-*",
        "Microsoft.CSharp": "4.0.1-beta-*"
      }
    },
    "dotnet": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.0",
        "System.Data.Common": "4.0.0",
        "System.Dynamic.Runtime": "4.0.10"
      }
    }
  }
}
