[
  {
    "name": "Sonar way",
    "language": "java",
    "default": true,
    "rules": [
      {
        "key": "DuplicatedBlocks",
        "repo": "common-java",
        "severity": "MAJOR"
      },
      {
        "key": "InsufficientBranchCoverage",
        "repo": "common-java",
        "severity": "MAJOR",
        "params": [
          {
            "key": "minimumBranchCoverageRatio",
            "value": "65.0"
          }
        ]
      },
      {
        "key": "S00105",
        "repo": "squid",
        "severity": "MINOR"
      },
      {
        "key": "MethodCyclomaticComplexity",
        "repo": "squid",
        "severity": "MAJOR",
        "params": [
          {
            "key": "max",
            "value": "10"
          }
        ]
      }
    ]
  }
]
